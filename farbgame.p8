pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init() 

sp1 = { x = 24,
        y = 16,
        mx = 0,
        my = 0}
sp2 = { x = 24,
        y = 104,
        mx = 0,
        my = 0}
cam = { x = 1,
        y = 1}
z1 =  { mx = 29,
        my = 12}
z2 =  { mx = 0, 
        my = 0}
mapl = {x=0,
		y=0,
		w=50,
		h=20}
wincount=0
end

function _update60()

--links
	if btnp(0) then 
		s = mget(sp1.mx-1+mapl.x, sp1.my+mapl.y)
		if not fget(s,0) then
			sp1.x = sp1.x - 8
		end 
	end
 
	if btnp(0) then 
		s = mget(sp2.mx-1+mapl.x, sp2.my+mapl.y)
		if not fget(s,0) then
			sp2.x = sp2.x - 8
		end
	end

--rechts
	if btnp(1) then 
		s = mget(sp1.mx+1+mapl.x, sp1.my+mapl.y)
		if not fget(s,0) then
			sp1.x = sp1.x + 8
		end 
	end
 
	if btnp(1) then 
		s = mget(sp2.mx+1+mapl.x, sp2.my+mapl.y)
		if not fget(s,0) then
			sp2.x = sp2.x + 8
		end
	end
	
--unten
	if btnp(3) then 
		s = mget(sp1.mx+mapl.x, sp1.my+1+mapl.y)
		if not fget(s,0) then
			sp1.y = sp1.y + 8
		end 
	end
 
	if btnp(3) then 
		s = mget(sp2.mx+mapl.x, sp2.my+1+mapl.y)
		if not fget(s,0) then
			sp2.y = sp2.y + 8
		end
	end
 
--oben
	if btnp(2) then 
		s = mget(sp1.mx+mapl.x, sp1.my-1+mapl.y)
		if not fget(s,0) then
			sp1.y = sp1.y - 8
		end 
	end
 
	if btnp(2) then 
		s = mget(sp2.mx+mapl.x, sp2.my-1+mapl.y) 
		if not fget(s,0) then
			sp2.y = sp2.y - 8
		end
	end
 
--ziel abfrage
--sp1 = 1
--sp2 = 2

--mset = (29,12,1)

	if btn(0, 1) then
		cam.x -= 1
	end

	if btn(1, 1) then
		cam.x += 1
	end

	if btn(2,1) then
		cam.y -= 1
	end

	if btn(3,1) then
		cam.y += 1
	end
--camera set to playermovement
	cam.x=(sp1.x+sp2.x)/2-63
	cam.y=(sp1.y+sp2.y)/2-63
	camera(cam.x, cam.y)

	sp1.mx = flr(sp1.x/8)
	sp1.my = flr(sp1.y/8)
	sp2.mx = flr(sp2.x/8)
	sp2.my = flr(sp2.y/8)

	s1 = mget(sp1.mx+mapl.x, sp1.my+mapl.y)
	s2 = mget(sp2.mx+mapl.x, sp2.my+mapl.y)
	
	if fget(s1,1) and fget(s2,2) then
		win = true
	end
	
	if btnp(5) and win==true then
		wincount += 1
		win=false
		mapl.y += 20	
		sp1.x = 24
		sp1.y = 16
		sp2.x = 24
		sp2.y = 104
	end
end

--[[function grounded()
  local grounded = false

  tile.mx = p1.x/8
  tile.my = p1.y/8
  
  tile.s  = mget(tile.mx,
                 tile.my)
  grounded = fget(tile.s,0)
  
 return grounded
end]]



function _draw()
	cls()
	map(mapl.x, mapl.y, 0, 0, mapl.w, mapl.h)
--rot=40
--blau=41
	spr(3, sp1.x, sp1.y, 1, 1)
	spr(4, sp2.x, sp2.y, 1, 1)

	if win == true and wincount!=2 then
		rectfill(0, 0, 280, 280,1)
		print ("geschafft",cam.x+50,cam.y+64,7)
		print ("next level (v)", cam.x+50, cam.y+ 73,7)  
		print ("restart level (c)", cam.x+50, cam.y+ 80,7)
	end
	
	if win==true and wincount==2 then
		rectfill(0, 0, 280, 280, 3)
		print ("you win!",cam.x+50,cam.y+64,7)
	end
end
